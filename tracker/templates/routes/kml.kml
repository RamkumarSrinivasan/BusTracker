<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://earth.google.com/kml/2.2">
<Document>
{% for observation in observations: %}
  <Placemark>
		<name>
			{{ observation.time|localdatetime }}
		</name>
		<Point>
			<coordinates>{{ observation.location.x }}, {{ observation.location.y }}</coordinates>
		</Point>
  </Placemark>
{% endfor %}
</Document>
</kml>
